{
    "name": "Phalcon Skeleton Applicaton",
    "description": "Phalcon application",
    "license": "BSD-3-Clause",
    "keywords": [
        "framework",
        "phalcon"
    ],

    "config": {
        "component-dir": "vendor"
    },

    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "carhartl/jquery-cookie",
                "version": "dev-master",
                "source": {
                    "url": "https://github.com/carhartl/jquery-cookie.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        }
    ],

    "require": {
        "php": ">=5.4",
        "phalcon/devtools": "dev-master",
        "twitter/bootstrap": "3.*",
        "components/jquery": "2.*",
        "leafo/lessphp": "*",
        "carhartl/jquery-cookie": "dev-master",
        "symfony/Console": "*",
        "phpunit/phpunit": "3.*"
    },

    "autoload": {
        "psr-4": {
            "Core\\": "vendor/Core/",
            "Test\\": "tests/",
            "Phalcon\\": "library/Phalcon/"
        }
    }
}
